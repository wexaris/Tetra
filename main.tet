
func main() {
    var a: Number = 2;
    var b: Number = 7;

    var c: Number = sum2(a, b);

    print(c);

    while c != 5  {
        if c < 5 {
            c += 1;
        } else {
            c -= 1;
        }
    }

    print(c);

    return;
}

func sum2(a: Number, b: Number) : Number {
    return a + b;
}

